{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "components/map/reviewForm.jsx", "components/map/map.jsx", "components/map/bathStats.jsx", "components/map/bathroomReview.jsx", "components/mainComponent.jsx", "components/userReviews/editReviewForm.jsx", "components/userReviews/userReview.jsx", "components/userReviews/userReviews.jsx", "components/authButtons.jsx"],
  "exclude": ["node_modules"]
}
